{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":[
      {
        "source": "/editform/:title/**",
        "destination": "/editform/[title]/[desc]/"
      },
      {
        "source": "/resultat/**",
        "destination": "/resultat/[id]/"
      },
      {
        "source": "/public.form/**",
        "destination": "/public.form/[id]/"
      }
    ]
  }
}
